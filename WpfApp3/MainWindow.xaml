<Window x:Class="WpfApp3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        mc:Ignorable="d"
        Title="Считалка" Height="450" Width="800">
    
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25*"/>
            <RowDefinition Height="195*"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Grid.Row="0" Background="#E0FFFF"/>
        <Button Grid.Row="0" Content="Вернуться" VerticalAlignment="Center" HorizontalAlignment="Right" Width="120" Height="30" FontSize="17" Margin="10 0 140 0" Click="btnBack"/>
        <Button Grid.Row="0" Content="Помощь" VerticalAlignment="Center" HorizontalAlignment="Right" Width="120" Height="30" FontSize="17" Margin="10 0 10 0"/>
        <Frame NavigationUIVisibility="Hidden" Grid.Row="1" x:Name="FrmMain" Source="/WpfApp3;component/Pages/OwnerPage.xaml"/>
    </Grid>
</Window>
